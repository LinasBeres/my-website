mixin info
  meta(name="author" content="Linas Berensa")
  meta(name="description" content="My home page that will store links and other stuff")
  link(rel="shortcut icon" href="images/vytis.jpg")

doctype html
html
  head
    meta(charset="utf-8")
    +info
    link(rel="stylesheet" href="bower_components/bootstrap/dist/css/bootstrap.min.css")
    link(rel="stylesheet" href="bower_components/bootstrap/dist/css/bootstrap-theme.min.css")
    link(rel="stylesheet" href="stylesheets/styles.css")
    block meta
      title Home Page


  body
    block body
    block scripts
      script(src="bower_components/jquery/dist/jquery.min.js")
      script(src="bower_components/bootstrap/dist/js/bootstrap.min.js")
      script(src="bower_components/jquery-ui/jquery-ui.min.js")
